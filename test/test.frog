import "frog::math";
import "ahoj";

print(ahoj("hyro"));

fun test(n) {
    let phi = (1.0 + math_sqrt(5))/2.0;
    let asymp = math_pow(phi, n) / math_sqrt(5);

    return math_floor(asymp);
}

let fibo = test(50);

if (fibo > 50.0) {
    print("Fibonacci number is greater than 50");
} else {
    print("Fibonacci number is less than 50");
}

print(fibo);